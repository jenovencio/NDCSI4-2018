
########################
# LINEAR SYSTEM SETTINGS
########################

# Number of physical degrees of freedom
>dof
2

# Mass matrix file path and type (full or sparse)
>mass
mass
full

# Damping matrix file path and type (full or sparse)
>damp
damping
full

# Stiffness matrix file path and type (full or sparse)
>stiff
stiffness
full

# Excitation force file path
>exc
excitation

##############
# HBM SETTINGS
##############

# Number of harmonic waves
>harm
16

# Number of integration points
>int
1000

#######################
# CONTINUATION SETTINGS
#######################

# Max continuation steps
>stepscont
500000

# Start frequency
>start
0.004

# End frequency
>end
10

# Max newton steps
>stepsnewt
6

# Newton norm F
>normnewt
1e-7

# Initial step size
>stepinit
1e-4

# Minimum step size
>stepmin
1e-11

# Maximum step size
>stepmax
4e-1

# Predictor type (Constant, Tangent, Secant)
>pred
Tangent

# Scale arc length
>scalearc
1

